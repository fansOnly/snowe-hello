{
	"presets": [
		["@babel/preset-env", {
			"modules": false,
			"targets": {
				"browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
			},
			"useBuiltIns": "usage"
		}]
	],
	"plugins": [
		"@babel/plugin-transform-runtime",
		"@vue/babel-plugin-transform-vue-jsx",
		"@babel/plugin-syntax-dynamic-import",
		["import", {
			"libraryName": "ant-design-vue",
			"libraryDirectory": "es",
			"style": "css" // `style: true` 会加载 less 文件
		}]
	]
}
